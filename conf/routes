# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index

POST    /api/articles               controllers.ArticleController.insertArticle


POST    /api/books                  controllers.BookController.insertBook
GET     /api/books                  controllers.BookController.getAllBooks


GET     /api/electronics            controllers.ElectronicController.getAllElectronics

#GET     /Books                      controllers.BookController.getAll
#POST    /Books                      controllers.BookController.insert



# Map static resources from the /public folder to the /assets URL path
# Needed for files in the public folder like js or stylesheets etc.
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
